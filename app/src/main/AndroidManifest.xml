<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nemurin.st0321.nemurin2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/aicon_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/aicon_launcher"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat">
        <activity
            android:name="com.nemurin.st0321.nemurin2.MainActivity"
            android:label="@string/app_name"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.nemurin.st0321.nemurin2.SettingsActivity"
            android:label="@string/title_activity_settings"
            android:parentActivityName="com.nemurin.st0321.nemurin2.MainActivity"></activity>
    </application>

</manifest>